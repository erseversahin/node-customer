{
  "swaggerDefinition": {
    "swagger": "2.0",
    "info": {
      "title": "Customer API +",
      "description": "Customer api with nodejs and express server.",
      "termsOfService": "https://stebilisim.com/",
      "contact": {
        "name": "Åžahin ERSEVER",
        "email": "info@stebilisim.com"
      },
      "servers": ["http://localhost:5000"]
    },
    "host": "localhost:5000",
    "basePath": "/api",
    "schemes": [
      "http"
    ],
    "securityDefinitions": {
      "BearerAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "schema": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "paths": {
      "/auth/login": {
        "post": {
          "security": {
            "Bearer": {
              "type": "apiKey",
              "name": "Authorization",
              "in": "header"
            }
          },
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [

            {
              "in": "body",
              "name": "body",
              "description": "Requests are authenticated using the Authorization header and value Bearer: {{token}}. with a valid JWT.",
              "required": true,
              "type": "String",
              "schema": {
                "required": [
                  "email",
                  "password"
                ],
                "properties" : {
                  "email": {
                    "type": "string",
                    "example": "ersversahint@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "123456"
                  }
                }
              }
            }
          ],

          "responses": {
            "200": {
              "description": "Will send `Authenticated`"
            },
            "401": {
              "description": "You do not have necessary permissions for the resource"
            }
          }
        }
      },
      "/user/profile/show": {
        "get": {
          "security": {
            "Bearer": {
              "type": "apiKey",
              "name": "Authorization",
              "in": "header"
            }
          },
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],


          "responses": {
            "200": {
              "description": "Will send `Authenticated`"
            },
            "401": {
              "description": "You do not have necessary permissions for the resource"
            }
          }
        }
      }
    }
  },
  "apis": [
    "routers/*.js"
  ]
}